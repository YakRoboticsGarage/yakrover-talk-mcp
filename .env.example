# ElevenLabs
ELEVENLABS_API_KEY=your_api_key_here
# ELEVENLABS_VOICE_ID: George (default)
ELEVENLABS_VOICE_ID=JBFqnCBsd6RMkjVDRZzb
# ELEVENLABS_MODEL_ID: eleven_turbo_v2_5 (low latency) | eleven_multilingual_v2 (quality)
ELEVENLABS_MODEL_ID=eleven_turbo_v2_5

# Audio
# AUDIO_PLAYER: pw-play | afplay | mpg123
AUDIO_PLAYER=pw-play
